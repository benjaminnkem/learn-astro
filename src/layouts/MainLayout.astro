---
import MainHead from "./MainHead.astro";
import "@fontsource-variable/dm-sans";
import "@fontsource/anton";

interface Props {
  title: string;
  description: string;
}

const { title, description } = Astro.props as Props;
---

<html lang="en">
  <MainHead {title} {description} />
  <body class="font-sans">
    <slot />
  </body>
</html>
